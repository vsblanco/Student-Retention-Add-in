<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <!-- It's a good practice to generate a new GUID for your add-in -->
  <Id>541b5174-b498-4e43-98a1-c116e3a3f78b</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Victor Blanco</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Student Retention"/>
  <Description DefaultValue="An add-in for tracking student retention tasks."/>
  <IconUrl DefaultValue="~remoteAppUrl/images/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="~remoteAppUrl/images/icon-80.png"/>
  <SupportUrl DefaultValue="https://www.contoso.com/help"/>
  <AppDomains>
    <AppDomain>https://www.contoso.com</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="~remoteAppUrl/taskpane/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <!-- Using the same Tab ID as your working version -->
            <CustomTab id="Victor.RetentionTab">
              <Label resid="RetentionTab.Label"/>
              <!-- Student Group -->
              <Group id="StudentGroup">
                <Label resid="StudentGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="StudentDetailsButton">
                  <Label resid="StudentDetails.Label"/>
                  <Supertip>
                    <Title resid="StudentDetails.Label"/>
                    <Description resid="StudentDetails.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.Details.16x16"/>
                    <bt:Image size="32" resid="Icon.Details.32x32"/>
                    <bt:Image size="80" resid="Icon.Details.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>studentDetails</FunctionName>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="ContactedButton">
                  <Label resid="Contacted.Label"/>
                  <Supertip>
                    <Title resid="Contacted.Label"/>
                    <Description resid="Contacted.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.Contacted.16x16"/>
                    <bt:Image size="32" resid="Icon.Contacted.32x32"/>
                    <bt:Image size="80" resid="Icon.Contacted.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>contacted</FunctionName>
                  </Action>
                </Control>
              </Group>
              <!-- Data Group -->
              <Group id="DataGroup">
                <Label resid="DataGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="CreateLdaButton">
                  <Label resid="CreateLDA.Label"/>
                  <Supertip>
                    <Title resid="CreateLDA.Label"/>
                    <Description resid="CreateLDA.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.CreateLDA.16x16"/>
                    <bt:Image size="32" resid="Icon.CreateLDA.32x32"/>
                    <bt:Image size="80" resid="Icon.CreateLDA.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>createLDA</FunctionName>
                  </Action>
                </Control>
              </Group>
              <!-- System Group -->
              <Group id="SystemGroup">
                <Label resid="SystemGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="AboutButton">
                  <Label resid="About.Label"/>
                  <Supertip>
                    <Title resid="About.Label"/>
                    <Description resid="About.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.About.16x16"/>
                    <bt:Image size="32" resid="Icon.About.32x32"/>
                    <bt:Image size="80" resid="Icon.About.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <!-- Added required TaskpaneId -->
                    <TaskpaneId>About</TaskpaneId>
                    <SourceLocation resid="About.Url"/>
                  </Action>
                </Control>
                <!-- ** NEW SETTINGS BUTTON ** -->
                <Control xsi:type="Button" id="SettingsButton">
                    <Label resid="Settings.Label" />
                    <Supertip>
                        <Title resid="Settings.Label" />
                        <Description resid="Settings.Tooltip" />
                    </Supertip>
                    <Icon>
                        <bt:Image size="16" resid="Icon.Settings.16x16" />
                        <bt:Image size="32" resid="Icon.Settings.32x32" />
                        <bt:Image size="80" resid="Icon.Settings.80x80" />
                    </Icon>
                    <Action xsi:type="ShowTaskpane">
                        <!-- Added required TaskpaneId -->
                        <TaskpaneId>Settings</TaskpaneId>
                        <SourceLocation resid="Settings.Url" />
                    </Action>
                </Control>
              </Group>
            </CustomTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="~remoteAppUrl/images/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="~remoteAppUrl/images/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="~remoteAppUrl/images/icon-80.png"/>
        <bt:Image id="Icon.Contacted.16x16" DefaultValue="~remoteAppUrl/images/contacted-icon.png"/>
        <bt:Image id="Icon.Contacted.32x32" DefaultValue="~remoteAppUrl/images/contacted-icon.png"/>
        <bt:Image id="Icon.Contacted.80x80" DefaultValue="~remoteAppUrl/images/contacted-icon.png"/>
        <bt:Image id="Icon.Details.16x16" DefaultValue="~remoteAppUrl/images/details-icon.png"/>
        <bt:Image id="Icon.Details.32x32" DefaultValue="~remoteAppUrl/images/details-icon.png"/>
        <bt:Image id="Icon.Details.80x80" DefaultValue="~remoteAppUrl/images/details-icon.png"/>
        <bt:Image id="Icon.CreateLDA.16x16" DefaultValue="~remoteAppUrl/images/create-lda-icon.png"/>
        <bt:Image id="Icon.CreateLDA.32x32" DefaultValue="~remoteAppUrl/images/create-lda-icon.png"/>
        <bt:Image id="Icon.CreateLDA.80x80" DefaultValue="~remoteAppUrl/images/create-lda-icon.png"/>
        <bt:Image id="Icon.About.16x16" DefaultValue="~remoteAppUrl/images/about-icon.png"/>
        <bt:Image id="Icon.About.32x32" DefaultValue="~remoteAppUrl/images/about-icon.png"/>
        <bt:Image id="Icon.About.80x80" DefaultValue="~remoteAppUrl/images/about-icon.png"/>
        <bt:Image id="Icon.Settings.16x16" DefaultValue="~remoteAppUrl/images/settings-icon.png" />
        <bt:Image id="Icon.Settings.32x32" DefaultValue="~remoteAppUrl/images/settings-icon.png" />
        <bt:Image id="Icon.Settings.80x80" DefaultValue="~remoteAppUrl/images/settings-icon.png" />
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://go.microsoft.com/fwlink/?LinkId=276812"/>
        <bt:Url id="Commands.Url" DefaultValue="~remoteAppUrl/commands/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="~remoteAppUrl/taskpane/taskpane.html"/>
        <bt:Url id="About.Url" DefaultValue="~remoteAppUrl/about/about.html"/>
        <bt:Url id="Settings.Url" DefaultValue="~remoteAppUrl/settings/settings.html" />
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Get started with your add-in!"/>
        <bt:String id="RetentionTab.Label" DefaultValue="Retention"/>
        <bt:String id="StudentGroup.Label" DefaultValue="Student"/>
        <bt:String id="DataGroup.Label" DefaultValue="Data"/>
        <bt:String id="SystemGroup.Label" DefaultValue="System"/>
        <bt:String id="Contacted.Label" DefaultValue="Contacted"/>
        <bt:String id="StudentDetails.Label" DefaultValue="Details"/>
        <bt:String id="CreateLDA.Label" DefaultValue="Create LDA"/>
        <bt:String id="About.Label" DefaultValue="About"/>
        <bt:String id="Settings.Label" DefaultValue="Settings" />
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Your add-in loaded successfully. Go to the 'Retention' tab to get started."/>
        <bt:String id="Contacted.Tooltip" DefaultValue="Mark student as contacted."/>
        <bt:String id="StudentDetails.Tooltip" DefaultValue="See student details."/>
        <bt:String id="CreateLDA.Tooltip" DefaultValue="Create LDA file."/>
        <bt:String id="About.Tooltip" DefaultValue="About this add-in."/>
        <bt:String id="Settings.Tooltip" DefaultValue="Configure add-in settings." />
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>
